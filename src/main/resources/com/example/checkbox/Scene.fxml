<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="351.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.checkbox.Controller">
   <children>
      <CheckBox fx:id="myCheckBox" layoutX="107.0" layoutY="267.0" mnemonicParsing="false" onAction="#change" text="ON/OFF">
         <font>
            <Font size="18.0" />
         </font>
      </CheckBox>
      <Label fx:id="myLabel" layoutX="138.0" layoutY="207.0" text="OFF">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ImageView fx:id="myImageView" fitHeight="157.0" fitWidth="119.0" layoutX="95.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@lightbulbOFF.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
